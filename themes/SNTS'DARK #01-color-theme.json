	"tokenColors": [
		{
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"string.comment"
			],
			"settings": {
				"foreground": "#6A7062"
			}
		},
		{
			"scope": [
				"keyword",
        "storage.type",
        "storage.modifier"
			],
			"settings": {
				"foreground": "#9A3438"
			}
		},
    {
			"scope": [
        "string",
				"string punctuation.section.embedded source",
        "punctuation.definition.string"
			],
			"settings": {
				"foreground": "#6B8EAD"
			}
		},
    {
			"scope": [
        "constant.numeric",
        "constant.language"
			],
			"settings": {
				"foreground": "#5F9E93"
			}
		},
    {
			"scope": [
        "entity.name.function",
        "support.function"
			],
			"settings": {
				"foreground": "#95709A"
			}
		},
    {
			"scope": [
        "entity.name.tag",
				"support.class.component",
        "entity.name.class",
        "entity.name.type.class",
        "support.class"
			],
			"settings": {
				"foreground": "#B5896F"
			}
		},
		{
			"scope": [
				"constant",
        "support.constant",
				"entity.name.constant",
				"variable.other.constant",
				"variable.language"
			],
			"settings": {
				"foreground": "#B5896F"
			}
		},
		{
			"scope": [
				"variable.parameter.function",
				"meta.jsx.children",
				"meta.block",
				"meta.tag.attributes",
				"meta.object.member",
				"meta.embedded.expression",
        "variable"
			],
			"settings": {
				"foreground": "#EFF8E2"
			}
		},
		{
			"scope": "invalid.broken",
			"settings": {
				"foreground": "#9A3438",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "invalid.deprecated",
			"settings": {
				"foreground": "#9A3438",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "invalid.illegal",
			"settings": {
				"foreground": "#9A3438",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "invalid.unimplemented",
			"settings": {
				"foreground": "#9A3438",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"markup.deleted",
				"meta.diff.header.from-file",
				"punctuation.definition.deleted"
			],
			"settings": {
				"foreground": "#6A7062",
				"background": "#440D0F"
			}
		},
		{
			"scope": [
				"markup.inserted",
				"meta.diff.header.to-file",
				"punctuation.definition.inserted"
			],
			"settings": {
				"background": "#5F9E9320"
			}
		},
		{
			"scope": [
				"markup.heading",
				"markup.heading entity.name"
			],
			"settings": {
				"foreground": "#B5896F",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "markup.quote",
			"settings": {
				"foreground": "#5F9E93"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		}
	]
